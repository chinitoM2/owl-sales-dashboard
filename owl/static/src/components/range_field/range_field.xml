<?xml version="1.0" encoding="UTF-8" ?>
<templates>
    <t t-name="owl.RangeField" owl="1">
        <div>
            <label for="customRange1" class="form-label"><span t-esc="props.value"/> <span t-esc="currency"/></label>
            <input type="range" class="form-range" id="customRange1" min="24000" max="112000" step="1000" t-on-change="(event)=>this.props.update(event.target.value)" t-att-value="props.value"/>
        </div>
    </t>
</templates>